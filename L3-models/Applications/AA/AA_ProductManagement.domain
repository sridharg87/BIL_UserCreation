            @t24:i (align=L, inputBehaviour=N, maxLength=30, sysNumber=15.0, core=true, businessType=A, t24Name=PC.PRD.PR)
        /* This field acts as a pointer to a defined set of conditions for the Property. */
        PRD_PROPERTY : T24BusinessTypes:A
            @i18n:Label (en=Property)
            @t24:i (align=L, inputBehaviour=I, maxLength=67, sysNumber=14.0, core=true, t24Name=PRD.PROPERTY)
        RESERVED11 : T24BusinessTypes:A
