/* A live file that holds the basic details for XML statement
This table will contain the details for both CAMT052 and CAMT053 messages
For CAMT053, the table will be updated during Close of Business
For CAMT052, the table will be updated either on authorization of DE.STATEMENT.REQUEST record or after the AC.XML.INTRMSTMT.GENERATE service */
    AC_XML_STMT_DATA
        @i18n:Description (en="")
        @t24:i (t24Name=AC.XML.STMT.DATA, level=F, type=L, allowedFunctions="L P S", localRefAllowed=false)
    {
/* ACCOUNT.CURRENCY
The Currency of the Account */
        ACCOUNT_CURRENCY : T24BusinessTypes:CCY
            @i18n:Label (en=Ccy)
            @t24:i (align=L, inputBehaviour=I, maxLength=3, sysNumber=4.0, core=true, t24Name=ACCOUNT.CURRENCY)
/* ACCOUNT.IBAN
This field holds the IBAN id of the Account holder */
        ACCOUNT_IBAN : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=2.0, core=true, t24Name=ACCOUNT.IBAN)
/* Account Name
The Account title of the Account Holder */
        ACCOUNT_NAME : T24BusinessTypes:A
            @i18n:Label (en="Portfolio Name")
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=3.0, core=true, t24Name=ACCOUNT.NAME)
        AVAILABLE_DATE -> byValue IX_XmlStmtPrinting:AC_XML_STMT_DATA__AVAILABLE_DATE *
            @t24:i (t24Name=AVAILABLE.DATE)
        CARRIER_ADDR_NO -> byValue IX_XmlStmtPrinting:AC_XML_STMT_DATA__CARRIER_ADDR_NO *
            @t24:i (t24Name=CARRIER.ADDR.NO)
/* CLOSING.CLEARED
The Closing Cleared balance of the Statement */
        CLOSING_CLEARED : T24BusinessTypes:AMT
            @t24:i (align=R, inputBehaviour=I, maxLength=19, sysNumber=11.0, core=true, t24Name=CLOSING.CLEARED)
/* CO.CODE
The Company Code of the Account record */
        CO_CODE : T24BusinessTypes:A
            @i18n:Label (en=Company)
            @t24:i (align=L, inputBehaviour=I, maxLength=10, sysNumber=17.0, core=true, t24Name=CO.CODE)
/* CUSTOMER.ID
The Customer Id of the Account for which CAMT is generated */
        CUSTOMER_ID : T24BusinessTypes:A
            @i18n:Label (en="Customer Id")
            @i18n:Tooltip (en="Identifies the Customer for the transaction.")
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=1.0, core=true, t24Name=CUSTOMER.ID)
/* Id of DE.STATEMENT.REQUEST record
This field will hold value for both CAMT052 and CAMT053 messages
For CAMT052, it will be updated on authorizing the request record
For CAMT053, it will be updated during Close of Business
The field will contain value only for the requests coming from DE.STATEMENT.REQUEST application */
        DE_STAT_REQ_ID : T24BusinessTypes:TEXT
            @t24:i (align=L, inputBehaviour=I, maxLength=15, sysNumber=20.0, core=true, t24Name=DE.STAT.REQ.ID)
        FWD_STMT_PRINTED : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=28.0, core=true, t24Name=FWD.STMT.PRINTED)
/* Id of AC.XML.INTERIM.STMT.HIST record
This field will contain value only for CAMT052 messages
This field will be updated on authorizing the DE.STATEMENT.REQUEST record or after AC.XML.INTRMSTMT.GENERATE service */
        HIST_ID : T24BusinessTypes:TEXT
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=21.0, core=true, t24Name=HIST.ID)
/* Id of the record
Id will be in the format of ACCOUNT-STMTDATE.FREQUENCY.COMPANYCODE-MESSAGETYPE
Message Type will be either CAMT052 or CAMT053 */
        ID -> IX_XmlStmtPrinting:AC_XML_STMT_DATA PK required
            @i18n:Label (en="Transaction Ref")
            @i18n:Tooltip (en="Transaction Reference")
            @t24:i (align=L, inputBehaviour=I, maxLength=60, sysNumber=0.0, core=true, businessType=A, t24Name="@ID")
/* LAST.STMT.NO
The Last Statement Number */
        LAST_STMT_NO : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=I, maxLength=35, sysNumber=7.0, core=true, t24Name=LAST.STMT.NO)
/* LIMIT.AMOUNT
The Available Limit amount at the time the Statement is being generated */
        LIMIT_AMOUNT : T24BusinessTypes:AMT
            @i18n:Label (en="Limit Amount")
            @i18n:Tooltip (en="It shows the limit amount")
            @t24:i (align=R, inputBehaviour=I, maxLength=19, sysNumber=14.0, core=true, t24Name=LIMIT.AMOUNT)
/* Status of the CAMT message
This field will be updated after the XML.TRANSFORMATION service
Value can be "T24XML.GENERATED" or "CAMTXML.GENERATED"
The value "CAMTXML.GENERATED" represents the successful generation of CAMT message */
        MESSAGE_STATUS : T24BusinessTypes:A
            @i18n:Label (en="Message Status")
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=18.0, core=true, t24Name=MESSAGE.STATUS)
        ONLINE_ACTUAL_BAL : T24BusinessTypes:A
            @i18n:Label (en="Online Actual Bal")
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=30.0, core=true, t24Name=ONLINE.ACTUAL.BAL)
/* OPEN.STMT.BALANCE
The Opening Balance of the Statement, which will be the Closing balance of the previous statement */
        OPEN_STMT_BALANCE : T24BusinessTypes:AMT
            @t24:i (align=R, inputBehaviour=I, maxLength=19, sysNumber=9.0, core=true, t24Name=OPEN.STMT.BALANCE)
        OPEN_STMT_DATA : T24BusinessTypes:D
            @t24:i (align=R, inputBehaviour=I, maxLength=11, sysNumber=8.0, core=true, t24Name=OPEN.STMT.DATA)
        OTHER_DETAILS -> byValue IX_XmlStmtPrinting:AC_XML_STMT_DATA__OTHER_DETAILS *
            @t24:i (t24Name=OTHER.DETAILS)
/* Period End
The Period end Date when the Statement is being generated */
        PERIOD_END_DATE : T24BusinessTypes:D
            @t24:i (align=R, inputBehaviour=I, maxLength=11, sysNumber=16.0, core=true, t24Name=PERIOD.END.DATE)
        RESERVED_1 : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=N, maxLength=35, sysNumber=34.0, core=true, t24Name=RESERVED.1)
        RESERVED_2 : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=N, maxLength=35, sysNumber=33.0, core=true, t24Name=RESERVED.2)
        RESERVED_3 : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=N, maxLength=35, sysNumber=32.0, core=true, t24Name=RESERVED.3)
/* STMT.DATE
The date of the Statement */
        STMT_DATE : T24BusinessTypes:D
            @i18n:Label (en="Statement Date")
            @t24:i (align=R, inputBehaviour=I, maxLength=11, sysNumber=5.0, core=true, t24Name=STMT.DATE)
/* STMT.FQU.TYPE
The Frequency type of the Statement */
        STMT_FQU_TYPE : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=6.0, core=true, t24Name=STMT.FQU.TYPE)
        STMT_NO : T24BusinessTypes:N
            @i18n:Label (en="Statement number")
            @t24:i (align=R, inputBehaviour=I, maxLength=10, sysNumber=29.0, core=true, t24Name=STMT.NO)
/* STMT.PRINTED or STMT2.PRINTED id
The link to the STMT.PRINTED/STMT2.PRINTED record */
        STMT_PRINTED_ID : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=15.0, core=true, t24Name=STMT.PRINTED.ID)
/* XML.MESSAGE
This field will be updated after the XML.TRANSFORMATION service
Contains the generated CAMT message */
        XML_MESSAGE : T24BusinessTypes:TEXT
            @t24:i (align=L, inputBehaviour=I, maxLength=0, sysNumber=19.0, core=true, t24Name=XML.MESSAGE)
/* Type of XML Statement
Value in this field can be CAMT052 or CAMT053 */
        XML_STMT_TYPE : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=I, maxLength=7, sysNumber=10.0, core=true, t24Name=XML.STMT.TYPE)
    }
