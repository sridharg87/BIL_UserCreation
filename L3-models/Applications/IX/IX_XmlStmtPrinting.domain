/* A file that holds the details about the failed CAMT message
This table will contain the details for both CAMT052 and CAMT053 messages
It will get updated whenever there is a problem encountered during generation of CAMT message */
    AC_XML_STMT_EXCEPTION
        @i18n:Description (en="")
        @t24:i (t24Name=AC.XML.STMT.EXCEPTION, level=F, type=H, allowedFunctions="A C D E H I L P Q R S", localRefAllowed=false)
    {
        AUDITOR_CODE : T24BusinessTypes:A
            @i18n:Label (en="Auditor Code")
            @i18n:Tooltip (en="Auditor Code")
            @t24:i (align=L, inputBehaviour=E, maxLength=16, sysNumber=21.0, core=true, t24Name=AUDITOR.CODE)
        AUDIT_DATE_TIME : T24BusinessTypes:RELTIME
            @i18n:Label (en="Audit Date Time")
            @i18n:Tooltip (en="Date and time when record was audited")
            @t24:i (align=L, inputBehaviour=E, maxLength=15, sysNumber=22.0, core=true, t24Name=AUDIT.DATE.TIME, mask="RDD DD  DD ##:##")
        AUTHORISER : T24BusinessTypes:A
            @i18n:Label (en=Authoriser)
            @i18n:Tooltip (en="Indicates the User who Authorises the record")
            @t24:i (align=L, inputBehaviour=N, maxLength=40, sysNumber=18.0, core=true, t24Name=AUTHORISER)
        CO_CODE -> ST_CompanyCreation:COMPANY
            @i18n:Label (en=Company)
            @t24:i (align=L, inputBehaviour=N, maxLength=11, sysNumber=19.0, core=true, businessType=A, t24Name=CO.CODE, mask="R##-###-####")
        CURR_NO : T24BusinessTypes:N
            @i18n:Label (en="Curr No")
            @t24:i (align=R, inputBehaviour=N, maxLength=4, sysNumber=15.0, core=true, t24Name=CURR.NO)
        DATE_TIME -> byValue IX_XmlStmtPrinting:AC_XML_STMT_EXCEPTION__DATE_TIME *
            @t24:i (t24Name=DATE.TIME)
        DEPT_CODE -> ST_Config:DEPT_ACCT_OFFICER
            @i18n:Label (en="Dept Code")
            @t24:i (align=R, inputBehaviour=N, maxLength=4, sysNumber=20.0, core=true, t24Name=DEPT.CODE)
        ERROR_SOURCE -> byValue IX_XmlStmtPrinting:AC_XML_STMT_EXCEPTION__ERROR_SOURCE *
            @t24:i (t24Name=ERROR.SOURCE)
/* Id of the record
Id will be in the format of ACCOUNT-STMTDATE.FREQUENCY.COMPANYCODE-MESSAGETYPE
Message Type will be either CAMT052 or CAMT053
It will be same as that of AC.XML.STMT.DATA id
When CAMT is generated for multiple recipients there will be a sequence number appended after message type in the id */
        EXCEPTION_ID -> IX_XmlStmtPrinting:AC_XML_STMT_EXCEPTION PK required
            @t24:i (align=L, inputBehaviour=I, maxLength=90, sysNumber=0.0, core=true, businessType=A, t24Name=EXCEPTION.ID)
        INPUTTER -> byValue IX_XmlStmtPrinting:AC_XML_STMT_EXCEPTION__INPUTTER *
            @t24:i (t24Name=INPUTTER)
/* Status of the CAMT message
Value can be ERROR or SUCCESS
The value "ERROR" denotes that the CAMT message has not been generated
The value "SUCCESS" indicates that the CAMT message has been generated on the resubmission */
        MESSAGE_STATUS : T24BusinessTypes:A
            @i18n:Label (en="Message Status")
            @i18n:Tooltip (en="Status of the Exception Log")
            @t24:i (align=L, inputBehaviour=N, maxLength=35, sysNumber=1.0, core=true, t24Name=MESSAGE.STATUS)
/* MSG.RESUBMIT
This field is used to resubmit the CAMT request for the messages that had failed
Inputtable only for the XML.TRANFORM error
Value can either be null or YES */
        MSG_RESUBMIT : IX_XmlStmtPrinting:AC_XML_STMT_EXCEPTION__MSG_RESUBMIT
            @i18n:Label (en="Message Resubmit")
            @i18n:Tooltip (en="Allows resubmitting of the xml message")
            @t24:i (align=L, inputBehaviour=N, maxLength=3, sysNumber=2.0, core=true, t24Name=MSG.RESUBMIT)
/* Date of resubmission of CAMT request
Holds the date on which the CAMT request was resubmitted */
        MSG_RESUBMIT_DATE : T24BusinessTypes:D
            @i18n:Label (en="Msg Resubmit dt")
            @i18n:Tooltip (en="Indicates the Date Message has been resubmitted")
            @t24:i (align=R, inputBehaviour=N, maxLength=11, sysNumber=3.0, core=true, t24Name=MSG.RESUBMIT.DATE)
        RECORD_STATUS : T24BusinessTypes:STAT
            @i18n:Label (en="Record Status")
            @t24:i (align=L, inputBehaviour=N, maxLength=4, sysNumber=14.0, core=true, t24Name=RECORD.STATUS)
        RESERVED_FIELDS_1 : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=N, maxLength=35, sysNumber=13.0, core=true, t24Name=RESERVED.FIELDS.1)
        RESERVED_FIELDS_2 : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=N, maxLength=35, sysNumber=12.0, core=true, t24Name=RESERVED.FIELDS.2)
        RESERVED_FIELDS_3 : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=N, maxLength=35, sysNumber=11.0, core=true, t24Name=RESERVED.FIELDS.3)
        RESERVED_FIELDS_4 : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=N, maxLength=35, sysNumber=10.0, core=true, t24Name=RESERVED.FIELDS.4)
/* STMT.DATE
Date on which CAMT request was given */
        STMT_DATE : T24BusinessTypes:N
            @i18n:Label (en="Statement Date")
            @t24:i (align=R, inputBehaviour=N, maxLength=0, sysNumber=9.0, core=true, t24Name=STMT.DATE)
    }
