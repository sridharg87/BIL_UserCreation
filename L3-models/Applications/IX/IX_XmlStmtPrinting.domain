    AC_XML_STMT_EXCEPTION__ERROR_SOURCE
    {
/* ERROR.MESSAGE
Message that conveys in detail the reason for the failure of CAMT message generation */
        ERROR_MESSAGE : T24BusinessTypes:A
            @i18n:Label (en="Error Message")
            @i18n:Tooltip (en="Indicates the reason as to why the message failed")
            @t24:i (align=L, inputBehaviour=N, maxLength=35, sysNumber=5.0, core=true, t24Name=ERROR.MESSAGE)
        ERROR_RESERVED1 : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=N, maxLength=35, sysNumber=8.0, core=true, t24Name=ERROR.RESERVED1)
/* ERROR.SOURCE
Source of the error which caused the CAMT message to fail
Value can be DATA.ASSEMBLY, INTERFACE, XML.TRANSFORM
DATA.ASSEMBLY indicates that the value for mandatory tags had been missing
XML.TRANSFORM indicates XML transformation error which may occur due to error in the XSLT */
        ERROR_SOURCE : IX_XmlStmtPrinting:AC_XML_STMT_EXCEPTION__ERROR_SOURCE__ERROR_SOURCE
            @i18n:Label (en="Error Source")
            @i18n:Tooltip (en="Indicates if the error is due to Data assembly, Transformation, Prodn etc")
            @t24:i (align=L, inputBehaviour=N, maxLength=20, sysNumber=4.0, core=true, t24Name=ERROR.SOURCE)
        TAG_NAME -> byValue IX_XmlStmtPrinting:AC_XML_STMT_EXCEPTION__ERROR_SOURCE__TAG_NAME *
            @t24:i (t24Name=TAG.NAME)
    }
