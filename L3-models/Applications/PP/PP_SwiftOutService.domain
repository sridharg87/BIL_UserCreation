            @i18n:Label (en="Clearing Reason Code")
            @i18n:Tooltip (en="Clearing Reason Code")
            @t24:i (align=L, inputBehaviour=I, maxLength=4, sysNumber=13.0, core=true, t24Name=ClearingReasonCode)
