            @i18n:Label (en="Clearing Action Status Code")
            @i18n:Tooltip (en="Clearing Action Status Code")
            @t24:i (align=L, inputBehaviour=I, maxLength=4, sysNumber=15.0, core=true, t24Name=ClearingActionStatusCode)
