        /* This is the total amount of the individual transactions; if not provided the system will calculate the total amount */
        DEBIT_AMOUNT : T24BusinessTypes:AMT
            @i18n:Label (en="Debit Amount")
            @t24:i (align=R, inputBehaviour=I, maxLength=18, sysNumber=4.0, core=true, t24Name=DEBIT.AMOUNT, typeModifiers="-ï£»3")
        /* This is the currency of the single debit account */
        DEBIT_CURRENCY -> ST_CurrencyConfig:CURRENCY required
            @i18n:Label (en="Debit Currency")
            @t24:i (align=L, inputBehaviour=I, maxLength=3, sysNumber=3.0, core=true, businessType=CCY, t24Name=DEBIT.CURRENCY)
        DEPT_CODE -> ST_Config:DEPT_ACCT_OFFICER
            @i18n:Label (en="Dept Code")
            @t24:i (align=R, inputBehaviour=N, maxLength=4, sysNumber=54.0, core=true, t24Name=DEPT.CODE)
/* This field controls if debit advice is required - - the option selected here will apply to all transactions part of the FT.BULKCR.OT
A maximum of 2 characters may be entered.

The following values are permitted: Y NO */
        DR_ADVICE_REQD_Y_N : FT_BulkProcessing:FT_BULKCR_OT__DR_ADVICE_REQD_Y_N
            @i18n:Label (en="Dr Advice Reqd Y N")
            @t24:i (align=L, inputBehaviour=I, maxLength=2, sysNumber=43.0, core=true, t24Name=DR.ADVICE.REQD.Y.N)
        /* This is the Debit Value date which will apply to all individual transactions part of the FT.BULKCR.OT */
        DR_VALUE_DATE : T24BusinessTypes:D
            @t24:i (align=R, inputBehaviour=I, maxLength=11, sysNumber=5.0, core=true, t24Name=DR.VALUE.DATE)
        INPUTTER -> byValue FT_BulkProcessing:FT_BULKCR_OT__INPUTTER *
            @t24:i (t24Name=INPUTTER)
        ORDERING_BANK -> byValue FT_BulkProcessing:FT_BULKCR_OT__ORDERING_BANK *
