            @i18n:Label (en="Sam Valuation")
            @i18n:Tooltip (en="Portfolio Valuation")
            @t24:i (align=R, inputBehaviour=N, maxLength=18, sysNumber=64.0, core=true, t24Name=SAM.VALUATION)
