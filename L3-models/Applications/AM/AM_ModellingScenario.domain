        AC_SELECTED -> byValue AM_ModellingScenario:AM_SCENARIO__AC_SELECTED *
            @t24:i (t24Name=AC.SELECTED)
        AUDITOR_CODE : T24BusinessTypes:A
            @i18n:Label (en="Auditor Code")
