        HISTORIC : EB_Logging:TEC_ITEMS__HISTORIC
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=7.0, core=true, t24Name=HISTORIC)
        INPUTTER -> byValue EB_Logging:TEC_ITEMS__INPUTTER *
            @t24:i (t24Name=INPUTTER)
/* Field to distinguish between technical and business related activities

