    /* The table IS.COMMODITY.POSITION is a record of complete delivery of the Asset or Commodity as captured in the application IS.CONTRACT.DELIVERY. */
    IS_COMMODITY_POSITION
        @i18n:Description (en="")
        @t24:i (t24Name=IS.COMMODITY.POSITION, level=F, type=L, allowedFunctions="L P S", localRefAllowed=false)
    {
        /* The Asset for which the delivery of goods is being tracked. */
        ASSET_REF : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=3.0, core=true, t24Name=ASSET.REF)
        /* The quantity available for sale. This quantity increases with the delivery and decreases with the reversal or sale. */
        AVAILABLE_QTY : T24BusinessTypes:R
            @t24:i (align=R, inputBehaviour=I, maxLength=15, sysNumber=7.0, core=true, t24Name=AVAILABLE.QTY)
        /* The Commodity for which the delivery of goods is being tracked. */
        COMMODITY -> IS_Config:IS_COMMODITY
            @t24:i (align=R, inputBehaviour=I, maxLength=35, sysNumber=2.0, core=true, t24Name=COMMODITY)
        DELIVERED_DATE -> byValue IS_Purchase:IS_COMMODITY_POSITION__DELIVERED_DATE *
            @t24:i (t24Name=DELIVERED.DATE)
/* ID of the table IS.COMMODITY.POSITION.
Id format:
1. <PURCHASE.ID> - <COMMODITY> - for Quantified Commodities
2. <PURCHASE.ID> - <COMMODITY> - <ASSET.ID> - for Named Quantities */
        IS_COMMODITY_POSITION_ID -> IS_Purchase:IS_COMMODITY_POSITION PK required
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=0.0, core=true, businessType=A, t24Name=IS.COMMODITY.POSITION.ID)
        /* The quantity of Asset or Commodity purchased in the Contract. */
        PURCHASE_QTY : T24BusinessTypes:R
            @t24:i (align=R, inputBehaviour=I, maxLength=15, sysNumber=4.0, core=true, t24Name=PURCHASE.QTY)
        /* The Contract Reference of the Asset through which the Asset is requested and purchased. */
        PURCHASE_REF -> IS_Purchase:IS_CONTRACT
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=1.0, core=true, businessType=A, t24Name=PURCHASE.REF)
        /* The quantity of Asset that are reversed out of the delivered quantity. When the goods delivered are reversed, the quantity available for sale also goes down with the same count. */
        REVERSED_QTY : T24BusinessTypes:R
            @t24:i (align=R, inputBehaviour=I, maxLength=15, sysNumber=8.0, core=true, t24Name=REVERSED.QTY)
        SALE_DATE -> byValue IS_Purchase:IS_COMMODITY_POSITION__SALE_DATE *
            @t24:i (t24Name=SALE.DATE)
        /* The total quantity of Asset or Commodity delivered out of the total purchase quantity. */
        TOT_DELIVERED_QTY : T24BusinessTypes:R
            @t24:i (align=R, inputBehaviour=I, maxLength=15, sysNumber=5.0, core=true, t24Name=TOT.DELIVERED.QTY)
        /* The total quantity of Asset or Commodity sold out of the total delivered quantity. */
        TOT_SOLD_QTY : T24BusinessTypes:R
            @t24:i (align=R, inputBehaviour=I, maxLength=15, sysNumber=6.0, core=true, t24Name=TOT.SOLD.QTY)
    }
