            @i18n:Tooltip (en="Indicates Account Officer responsible")
            @t24:i (align=R, inputBehaviour=N, maxLength=4, core=false, t24Name=ACCOUNT.OFFICER, genOperation="CONTRACT.ID>SWAP>ACCOUNT.OFFICER")
        ACCR_FROM_DATE -> byValue SW_Contract:SWAP_BALANCES__ACCR_FROM_DATE *
            @t24:i (t24Name=ACCR.FROM.DATE)
        AMORT_SCHEDULE -> byValue SW_Contract:SWAP_BALANCES__AMORT_SCHEDULE *
