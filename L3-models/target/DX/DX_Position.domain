            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=1.0, core=true, t24Name=POSITION.ID)
    }
/* This is a link file which gets updated only when multiple exotic events are found at trade level.
As when multiple exotics are available the market price key becomes lengthy and unmanageable hence to avoid it the reference updated to XREF field in it replaces the OPTION.TYPE position of DX.MARKET.PRICE key. */
    DX_POS_PRICE_XREF_LINK
        @i18n:Description (en="")
        @t24:i (t24Name=DX.POS.PRICE.XREF.LINK, level=I, type=L, allowedFunctions="L P S", localRefAllowed=false)
    {
        KEY_FIELDS -> byValue DX_Position:DX_POS_PRICE_XREF_LINK__KEY_FIELDS *
            @t24:i (t24Name=KEY.FIELDS)
        /* Holds the maturity date which is one of the component of the key. */
        MATURITY_DATE : T24BusinessTypes:D
            @i18n:Label (en="Maturity Date")
            @i18n:Tooltip (en="Indicates the Maturity Date of the Transaction")
            @t24:i (align=R, inputBehaviour=I, maxLength=11, sysNumber=5.0, core=true, t24Name=MATURITY.DATE)
        /* ID of this file. Whose format is “ContractCode/TradeCurrency/MaturityDate/CallPut/StrikePrice/DeliveryCurrency/OptionStyle”. */
        POS_PRICE -> DX_Position:DX_POS_PRICE_XREF_LINK PK required
            @t24:i (align=L, inputBehaviour=I, maxLength=120, sysNumber=0.0, core=true, businessType=A, t24Name=POS.PRICE)
        RESERVED_1 : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=35, sysNumber=6.0, core=true, t24Name=RESERVED.1)
    }
    DX_POS_PRICE_XREF_LINK__KEY_FIELDS
    {
        /* Holds the exotic details with option type and user fields. Whose format will be “ExoticType1#UsrFldVal1*UsrFldVal2/ExoticType2#UsrFldVal1*UsrFldVal2$”. */
        KEY_FIELDS : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=I, maxLength=120, sysNumber=1.0, core=true, t24Name=KEY.FIELDS)
        RESERVED_2 : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=35, sysNumber=4.0, core=true, t24Name=RESERVED.2)
        RESERVED_3 : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=35, sysNumber=3.0, core=true, t24Name=RESERVED.3)
        /* This is the reference which replaces the OPTION.TYPE in DX.MARKET.PRICE. This is an incremental number along with contract code whose format is “ContractCode#Incremental”. */
        XREF : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=I, maxLength=120, sysNumber=2.0, core=true, t24Name=XREF)
    }
