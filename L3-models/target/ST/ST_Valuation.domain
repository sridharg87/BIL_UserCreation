            @t24:i (align=R, inputBehaviour=I, maxLength=9, sysNumber=32.0, core=true, t24Name=SELL.OUT.MARGIN)
        SHORT_DESCR -> byValue ST_Valuation:SUB_ASSET_TYPE__SHORT_DESCR *
