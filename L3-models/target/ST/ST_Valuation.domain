        PRICE_TOLERANCE : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=4, sysNumber=8.0, core=true, t24Name=PRICE.TOLERANCE)
        RECORD_STATUS : T24BusinessTypes:STAT
