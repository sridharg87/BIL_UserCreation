            @i18n:Label (en="Investment Pgm")
            @i18n:Tooltip (en="Investment Pgm used for Rebalancing")
            @t24:i (align=L, inputBehaviour=I, maxLength=12, sysNumber=18.0, core=true, businessType=A, t24Name=INVESTMENT.PROGRAM)
