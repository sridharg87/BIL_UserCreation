            @t24:i (align=L, inputBehaviour=N, maxLength=4, sysNumber=13.0, core=true, t24Name=RECORD.STATUS)
/* Reserved for future use.
This is a no input field. */
        RESERVED_2 : T24BusinessTypes:N
